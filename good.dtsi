cot {
    manifests {
        compatible = "arm, cert-descs";
        trusted_boot_fw_cert: trusted_boot_fw_cert {
        };

        tb_fw_config_hash: tb_fw_config_hash {
        };

        hw_config_hash: hw_config_hash {
        };

        fw_config_hash: fw_config_hash {
            
        };
    };
