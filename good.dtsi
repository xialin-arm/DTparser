	compatible = "arm, non-volatile-counter";

	#address-cells = <1>;
	#size-cells = <0>;

	trusted_nv_ctr: trusted_nv_ctr {
	};

	non_trusted_nv_ctr: non_trusted_nv_ctr {
	};
};